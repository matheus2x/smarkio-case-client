{
	"env": {
		"browser": true,
    "commonjs": true
  },
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["import", "prettier"],
	"rules": {
		"import/prefer-default-export": "off",
		"consistent-return": "warn",
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"import/first": "off",
		"no-console": "warn",
		"no-alert": "off"
	}
}
